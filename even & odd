import java.util.*;
class Numbers
{
public static void main(String args[])
{
int n=12345;
Scanner sc=new Scanner(System.in);
System.out.println("Enter the value");
n=sc.nextInt();
switch(n)
{
case 1 :
case 3 :
case 5 : System.out.println(" It is odd ");
break;
case 2 :
case 4 : System.out.println(" It is even ");
break;
}}}
