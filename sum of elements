class SumOfElements
{
     static int a[] = {3,45,5,77};
     static int sum()
     {
         int sum = 0;
         int i;
         for (i = 0; i < a.length; i++)
            sum +=  a[i];
         return sum;
     }
     public static void main(String[] args) 
     {
         System.out.println("Sum of given array is " + sum());
        }
 }
